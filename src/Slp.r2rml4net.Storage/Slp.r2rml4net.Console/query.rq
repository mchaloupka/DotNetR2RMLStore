PREFIX gr: <http://purl.org/goodrelations/v1#>
PREFIX ruian: <http://ruian.linked.opendata.cz/ontology/>
PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX ec: <http://ec.europa.eu/eurostat/ramon/ontologies/geographic.rdf#>
PREFIX s: <http://schema.org/>
PREFIX dcterms: <http://purl.org/dc/terms/>

select *
where
{
?ca a s:CheckAction;
s:location/s:location ?region;
s:location/s:geo ?geo;
s:object ?object;
dcterms:date ?date ;
s:result ?result.
?result a <http://linked.opendata.cz/ontology/coi.cz/Sanction>;
s:result/gr:hasCurrencyValue ?value.
?object gr:legalName ?title .
?region a ec:LAURegion;
ec:level 2.
?geo s:latitude ?lat;
s:longitude ?lon.
}