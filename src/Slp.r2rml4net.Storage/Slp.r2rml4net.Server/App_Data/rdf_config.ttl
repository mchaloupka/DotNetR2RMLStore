@prefix dnr: <http://www.dotnetrdf.org/configuration#> .

<dotnetrdf:/sparql> a dnr:HttpHandler ;
  dnr:type "VDS.RDF.Query.QueryHandler" ;
  dnr:queryProcessor _:proc .
  
_:proc a dnr:SparqlQueryProcessor ;
  dnr:type "VDS.RDF.Query.GenericQueryProcessor" ;
  dnr:storageProvider _:manager .
 
_:manager a dnr:StorageProvider ;
  dnr:type "Slp.r2rml4net.Storage.R2RMLStorage, Slp.r2rml4net.Storage, Version=1.0.0.0, Culture=neutral, PublicKeyToken=null" .

<dotnetrdf-configure:VDS.RDF.Options#InternUris> dnr:configure false .